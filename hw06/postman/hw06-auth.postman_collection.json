					"script": {
						"exec": [
							"pm.test(\"Status code is 200\", function () {",
							"    pm.response.to.have.status(200);",
							"});",
							"",
							"pm.collectionVariables.set(\"id1\", pm.response.json().id);",
							"//console.log(pm.environment.get(\"id1\"));"
						],
						"type": "text/javascript"
					}
				},
				{
					"listen": "prerequest",
					"script": {
						"exec": [
							"function randomString(minValue, maxValue, dataSet = 'abcdefghijklmnopqrstuvwxyz0123456789ABCDEFGHIJKLMNOPQRSTUVWXYZ') {",
							"    if (!minValue) {",
							"        minValue = 10;",
							"        maxValue = 10;",
							"    }",
							"",
							"    if (!maxValue) {",
							"        maxValue = minValue;",
							"    }",
							"",
							"    let length = _.random(minValue, maxValue),",
							"        randomString = \"\";",
							"",
							"    for (let i = 0; i < length; i++)",
							"        randomString += dataSet.charAt(Math.floor(Math.random() * dataSet.length));",
							"    return randomString;",
							"}",
							"",
							"pm.collectionVariables.set(\"user1\", randomString());",
							"pm.collectionVariables.set(\"pass1\", randomString());",
							"",
							"//console.log(pm.collectionVariables.get(\"user1\"));",
							"//console.log(pm.collectionVariables.get(\"pass1\"));"
						],
						"type": "text/javascript"
					}
				}
			],
